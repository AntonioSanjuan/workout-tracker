{
  "name": "@workout-tracker/source",
  "version": "0.0.6",
  "license": "MIT",
  "scripts": {
    "build:all": "nx build shell --verbose",
    "start:all": "nx serve shell --open --devRemotes=account,workout-exercise-templates,workout-trainings --verbose",
    "build:workout-exercise-templates": "nx build workout-exercise-templates --verbose",
    "test:all": "nx run-many --all --target=test --no-cache --coverage=true",
    "test:app:shell": "nx run-many -t test --p shell --watch --no-cache --coverage=true",
    "test:app:account": "nx run-many -t test --p account --watch --no-cache --coverage=true",
    "test:app:workout-exercise-templates": "nx run-many -t test --p workout-exercise-templates --watch --no-cache --coverage=true",
    "test:app:workout-trainings": "nx run-many -t test --p workout-trainings --watch --no-cache --coverage=true",
    "test:lib:shared-store": "nx run-many -t test --p libs/shared-store --watch --no-cache --coverage=true",
    "test:lib:service:culture": "nx run-many -t test --p libs/services/culture --watch --no-cache --coverage=true",
    "test:lib:service:auth": "nx run-many -t test --p libs/services/auth --watch --no-cache --coverage=true",
    "test:lib:service:auth-persistance": "nx run-many -t test --p libs/services/auth-persistance --watch --no-cache --coverage=true",
    "test:lib:service:message": "nx run-many -t test --p libs/services/message --watch --no-cache --coverage=true",
    "test:lib:service:user-settings": "nx run-many -t test --p libs/services/user-settings --watch --no-cache --coverage=true",
    "test:lib:service:exercises": "nx run-many -t test --p libs/services/exercise-templates --watch --no-cache --coverage=true",
    "predeploy": "npm run build:all -- --prod --baseHref=/workout-tracker/",
    "deploy": "gh-pages -d dist/workout-tracker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.1.0",
    "@angular/common": "~17.1.0",
    "@angular/compiler": "~17.1.0",
    "@angular/core": "~17.1.0",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "~17.1.0",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "~17.1.0",
    "@angular/platform-browser-dynamic": "~17.1.0",
    "@angular/router": "~17.1.0",
    "@ngrx/component": "^17.1.0",
    "@ngrx/component-store": "~17.0.0",
